; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="USPET DO PARI"
run/main_scene="res://presents.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/image="res://STM_STARTPAGE.png"
boot_splash/fullsize=false
config/icon="res://icon.svg"

[autoload]

Singletone="*res://singletone.gd"
Dialogic="*res://addons/dialogic/Core/DialogicGameHandler.gd"

[dialogic]

directories/dch_directory={
"AlexandrUntik": "res://Dialogs/AlexandrUntik.dch",
"AndreiKulek": "res://Dialogs/AndreiKulek.dch",
"DanilNavrakin": "res://Dialogs/DanilNavrakin.dch"
}
directories/dtl_directory={
"Bossfight": "res://Dialogs/Bossfight.dtl",
"TegovirOpen": "res://Dialogs/TegovirOpen.dtl",
"VMetro": "res://Dialogs/VMetro.dtl",
"Victory": "res://Dialogs/Victory.dtl"
}
glossary/default_case_sensitive=true
layout/style_list=[]
layout/default_style="Default"
variables={}
text/letter_speed=0.01
text/initial_text_reveal_skippable=true
text/text_reveal_skip_delay=0.1
text/autoadvance_per_character_delay=0.1
text/autoadvance_ignored_characters_enabled=true
animations/join_default_length=0.5
animations/leave_default_length=0.5
animations/leave_default_wait=true
animations/join_default_wait=true
choices/autofocus_first=true
choices/delay=0.2
choices/reveal_delay=0
choices/reveal_by_input=false
save/autosave_delay=60.0
save/encryption_on_exports_only=true
extensions_folder="res://addons/dialogic_additions"
text/autopauses={}
glossary/glossary_files=["res://Dialogs/New_File.tres"]

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/stretch/mode="canvas_items"

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogic/plugin.cfg")

[importer_defaults]

animation_library={
"animation/fps": 60.0
}

